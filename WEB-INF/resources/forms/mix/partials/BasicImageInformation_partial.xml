<?xml version="1.0" encoding="UTF-8"?>
<!-- Default partial to show mods:identifer --> 
<html xmlns="http://www.w3.org/1999/xhtml" xmlns:xhtml="http://www.w3.org/1999/xhtml"
    xmlns:xforms="http://www.w3.org/2002/xforms" xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xxi="http://orbeon.org/oxf/xml/xinclude"
    xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xxforms="http://orbeon.org/oxf/xml/xforms"
    xmlns:mix="http://www.loc.gov/mix/v20" xmlns:fr="http://orbeon.org/oxf/xml/form-runner"
    xmlns:dl="http://dl.lib.brown.edu/editor/mods/dl" xmlns:xlink="http://www.w3.org/1999/xlink">

    <!-- BEGIN IDENTIFIER -->
    <!-- MODS:IDENTIFIER  -->
    <fr:section id="BasicImageInformation-section" bind="fr-form-binds">
        <xforms:label ref="$form-resources/detail/titles/BasicImageInformation"/>
        <xforms:help ref="$form-resources/detail/help/BasicImageInformation"/>
        <xforms:group nodeset="mix:BasicImageInformation/mix:BasicImageCharacteristics">
            <fr:body>

                <fr:tr>
                    <fr:td>

                        <xforms:input ref="mix:imageWidth">
                            <xforms:label ref="$form-resources/detail/labels/imageWidth"/>
                            <xforms:alert ref="$form-resources/detail/alerts/imageWidth"/>
                        </xforms:input>

                    </fr:td>
                    <fr:td>
                        <xforms:input incremental="true" ref="mix:imageHeight">
                            <xforms:label ref="$form-resources/detail/labels/imageHeight"/>
                            <xforms:alert ref="$form-resources/detail/alerts/imageHeight"/>
                        </xforms:input>
                    </fr:td>
                    <fr:td>
                        <xforms:input incremental="true" ref="mix:PhotometricInterpretation/mix:colorSpace">
                            <xforms:label ref="$form-resources/detail/labels/colorSpace"/>
                            <xforms:alert ref="$form-resources/detail/alerts/colorSpace"/>
                            <xforms:help ref="$form-resources/detail/help/colorSpace"/>
                        </xforms:input>
                    </fr:td>
                </fr:tr>
            </fr:body>
        </xforms:group>
    </fr:section>
    <!-- END IDENTIFIER -->
